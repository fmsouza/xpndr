{
  "name": "xpndr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:build": "cd android && ./gradlew assembleRelease && cd .. && cp android/app/build/outputs/apk/release/app-release.apk app-release.apk",
    "android:logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "upgrade": "react-native upgrade",
    "clean": "npx react-native-clean-project",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@hookform/resolvers": "^2.8.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/elements": "^1.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "color": "^4.2.1",
    "eslint-plugin-jest": "^26.1.1",
    "graphql": "^16.3.0",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-hook-form": "^7.28.1",
    "react-native": "^0.67.4",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-keychain": "^8.0.0",
    "react-native-safe-area-context": "4.0.1",
    "react-native-screens": "^3.12.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-vector-icons": "^9.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/runtime": "7.17.8",
    "@react-native-community/eslint-config": "3.0.1",
    "@types/color": "^3.0.3",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/react-native": "0.67.3",
    "@types/react-native-svg-charts": "^5.0.12",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "babel-jest": "27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "8.11.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "metro-react-native-babel-preset": "0.69.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-vector-icons)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  }
}
